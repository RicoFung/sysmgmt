	<properties>
		<spring.boot.version>1.5.3.RELEASE</spring.boot.version>
		<mybatis.spring.boot.version>1.3.0</mybatis.spring.boot.version>
		<log4j.version>2.6.2</log4j.version>
	</properties>
	 -->
	
